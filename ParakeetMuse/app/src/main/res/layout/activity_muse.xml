<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.medical.parakeet.parakeet.MuseActivity"
    android:id="@+id/rl1"
    android:background="#00ff00">

    <TextView android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/connect"
        android:text="Connection Status:"
        android:textColor="#000000"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="unknown"
        android:id="@+id/con_status"
        android:layout_below="@+id/connect"
        android:layout_alignParentStart="true"
        android:textColor="#000000" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Connect"
        android:id="@+id/con_button"
        android:layout_alignTop="@+id/connect"
        android:layout_alignParentEnd="true"
        android:onClick="conButton"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Alpha Sessional"
        android:id="@+id/alpha_sesh"
        android:layout_below="@+id/con_button"
        android:layout_alignParentStart="true"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0.0"
        android:id="@+id/alpha_sesh0"
        android:layout_below="@+id/alpha_sesh"
        android:layout_alignParentStart="true"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0.0"
        android:id="@+id/alpha_sesh3"
        android:layout_alignTop="@+id/alpha_sesh0"
        android:layout_toEndOf="@+id/con_status"
        android:textColor="#000000" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Delta Sessional"
        android:id="@+id/delta_sesh"
        android:layout_below="@+id/alpha_sesh0"
        android:layout_alignParentStart="true"
        android:layout_marginTop="29dp"
        android:textColor="#000000"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0.0"
        android:id="@+id/delta_sesh0"
        android:layout_below="@+id/delta_sesh"
        android:layout_alignParentStart="true"
        android:textColor="#000000"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0.0"
        android:id="@+id/delta_sesh3"
        android:layout_below="@+id/delta_sesh"
        android:layout_toEndOf="@+id/con_status"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Theta Sessional"
        android:id="@+id/theta_sesh"
        android:layout_below="@+id/delta_sesh0"
        android:layout_alignParentStart="true"
        android:layout_marginTop="29dp"
        android:textColor="#000000"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0.0"
        android:id="@+id/theta_sesh0"
        android:layout_below="@+id/theta_sesh"
        android:layout_alignParentStart="true"
        android:textColor="#000000" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Disconnect"
        android:id="@+id/discon_button"
        android:layout_below="@+id/con_button"
        android:layout_alignParentEnd="true"
        android:onClick="disconButton"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EEG"
        android:id="@+id/EEG"
        android:layout_below="@+id/theta_sesh0"
        android:layout_alignParentStart="true"
        android:layout_marginTop="18dp"
        android:textColor="#000000"/>
    <LinearLayout
        android:layout_below="@+id/EEG"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_weight="1"
        android:orientation="horizontal" >
        <TextView
            android:id="@+id/eeg_tp9"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textSize="16sp"
            android:text="0.0"
            android:textColor="#000000" />
        <TextView
            android:id="@+id/eeg_fp1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textSize="16sp"
            android:text="0.0"
            android:textColor="#000000"/>
        <TextView
            android:id="@+id/eeg_fp2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textSize="16sp"
            android:text="0.0"
            android:textColor="#000000"/>
        <TextView
            android:id="@+id/eeg_tp10"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textSize="16sp"
            android:text="0.0"
            android:textColor="#000000" />
    </LinearLayout>

</RelativeLayout>
