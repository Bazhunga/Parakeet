<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.medical.parakeet.parakeet" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.NoActionBar" >
        <activity
            android:name=".Main_PatientDetail"
            android:label="@string/app_name"
            android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TestBeacons"
            android:label="@string/title_activity_test_beacons"
            android:parentActivityName=".Main_PatientDetail" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.medical.parakeet.parakeet.Main_PatientDetail" />
        </activity>
        <activity
            android:name=".LeDeviceListAdapter"
            android:label="@string/title_activity_le_device_list_adapter" >
        </activity>
    </application>

</manifest>
